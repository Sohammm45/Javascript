<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Multiplication Table</title>
</head>
<body>

    <script>
        const number = parseInt(prompt("Enter a number:"));
        let table = "";

        for (let i = 1; i <= 10; i++) {
            table += number + " x " + i + " = " + (number * i) + "\n";
        }

        alert(table);
    </script>

</body>
</html>
